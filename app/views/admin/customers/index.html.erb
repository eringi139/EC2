<h2>会員一覧</h2>

<div>
  <table>
    <thead>
      <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </tr>
    </thead>
<% @customers.each do |customer| %>
<tbody>
  <tr>
  <td><%= customer.id %></td>
  <td><%= customer.last_name %><%= customer.first_name %></td>
  <td><%= customer.email %></td>
  </tr>
</tbody>
<% end %>
</table>
</div>